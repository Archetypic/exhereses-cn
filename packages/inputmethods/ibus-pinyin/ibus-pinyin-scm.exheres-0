require ibus-pinyin

PLATFORMS="~amd64 ~x86"
DEPENDENCIES="
    build:
        gnome-desktop/gnome-common
"

src_prepare() {
    echo -e "data/db/android/pydict.py\nscripts/pydict.py" >> po/POTFILES.in
    NOCONFIGURE=1 edo ./autogen.sh
    ibus-pinyin_src_prepare
}

